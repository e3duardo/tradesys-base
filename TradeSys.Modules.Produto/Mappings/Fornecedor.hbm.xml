<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="TradeSys.Modules.Produto" namespace="TradeSys.Modules.Produto.Domain"> 
  
  <class name="FornecedorModel" table="[Fornecedor]">

    <id name="Id" column="IdFornecedor">
      <generator class="native"/>
    </id>

    <property name="Nome"/>
    <property name="Sobrenome"/>
    <property name="Bairro"/>
    <property name="Cep"/>
    <property name="Cidade"/>
    <property name="Complemento"/>
    <property name="Estado"/>
    <property name="Logadouro"/>
    <property name="Email"/>

    <property name="Sys_DataCadastro"/>
    <property name="Sys_DataModificado"/>
    <property name="Sys_Ativo"/>
    
  </class>
  
</hibernate-mapping>